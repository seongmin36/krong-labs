{
  "name": "three-core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --out-dir dist --clean",
    "dev": "tsup src/index.ts --format esm --dts --out-dir dist --watch"
  },
  "dependencies": {
    "three": "0.182.0"
  },
  "devDependencies": {
    "@types/three": "^0.182.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
